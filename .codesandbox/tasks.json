{
  // These tasks will run in order when initializing your CodeSandbox project.
  "setupTasks": [
    {
      "name": "Install Dependencies",
      "command": "npm install"
    }
  ],

  // These tasks can be run from CodeSandbox. Running one will open a log in the app.
  "tasks": {
    "dev": {
      "name": "dev",
      "command": "npm run dev",
      "runAtStart": true,
      "preview": {
        "port": 3000
      }
    },
    "build": {
      "name": "build",
      "command": "npm run build"
    },
    "start": {
      "name": "start",
      "command": "npm run start"
    },
    "generate": {
      "name": "generate",
      "command": "npm run generate"
    },
    "emulator:hosting": {
      "name": "emulator:hosting",
      "command": "npm run emulator:hosting"
    },
    "lint:fix": {
      "name": "lint:fix",
      "command": "npm run lint:fix"
    },
    "lint": {
      "name": "lint",
      "command": "npm run lint"
    },
    "test": {
      "name": "test",
      "command": "npm run test"
    },
    "test:coverage": {
      "name": "test:coverage",
      "command": "npm run test:coverage"
    },
    "lint-staged": {
      "name": "lint-staged",
      "command": "npm run lint-staged"
    },
    "prepare": {
      "name": "prepare",
      "command": "npm run prepare"
    },
    "prepare-csp-nonce": {
      "name": "prepare-csp-nonce",
      "command": "npm run prepare-csp-nonce"
    },
    "deploy": {
      "name": "deploy",
      "command": "npm run deploy"
    }
  }
}
